{
  "manifest_version": 3,
  "name": "Bookmark All Windows",
  "version": "1.0.0",
  "description": "Bookmark all tabs from all windows at once into organized folders",
  "permissions": [
    "bookmarks",
    "tabs"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
